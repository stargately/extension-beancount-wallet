{
  "name": "chrome-extension-boilerplate-react",
  "version": "2.7.0",
  "description": "A chrome extension boilerplate built with React 16 and Webpack 4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lxieyang/chrome-extension-boilerplate-react.git"
  },
  "scripts": {
    "build": "node utils/build.js",
    "start": "node utils/webserver.js",
    "ava": "NODE_ENV=test ava",
    "prettier": "prettier --write '**/*.{js,jsx,css,html}'",
    "lint": "pretty-quick --staged && eslint src --fix --ext .js,.tsx,.ts --quiet",
    "test": "npm run lint && tsc && npm run ava",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recess-order",
      "stylelint-config-prettier"
    ],
    "rules": {
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/react": "^16.9.26",
    "@types/react-router-config": "^5.0.1",
    "antd": "^4.6.3",
    "iotex-antenna": "^0.30.1",
    "onefx": "^2.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "recoil": "^0.0.10",
    "styletron-engine-atomic": "1.4.6",
    "styletron-react": "5.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@peculiar/webcrypto": "^1.1.3",
    "@puncsky/eslint-config-onefx": "^2.3.7",
    "@puncsky/eslint-config-onefx-react": "^2.3.7",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/chrome": "0.0.104",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "ava": "3.11.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "browser-env": "^3.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "7.7.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-react": "7.20.6",
    "eth-keyring-controller": "^6.1.0",
    "file-loader": "^6.0.0",
    "fs-extra": "^9.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^4.0.2",
    "husky": "^4.2.5",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.2",
    "pretty-quick": "^3.0.0",
    "react-is": "^16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-require-connnect-typing": "^1.0.1",
    "typescript": "^4.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "3.2.1",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
